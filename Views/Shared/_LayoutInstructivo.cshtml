@using System

@{
    ViewBag.Title = "EmPoderTIC";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewBag.EmPoderTIC</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <!-- Core theme CSS (includes Bootstrap)-->
    <!-- Fuentes - Tipo de letra - Iconografía -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css">
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@300;400;700&display=swap" rel="stylesheet">
    <link href="~/Content/css/footer.css" rel="stylesheet" />
    @RenderSection("css", required: false)
    @RenderSection("js", required: false)
</head>
<body>

    <!-- Contenido -->
    @RenderBody()

    <footer>
        <div class="footer-line"></div>
        <img class="footer-logo" src="~/Content/img/footer.png" alt="Logo">
        <div class="footer-line"></div>
        <div class="footer-icons">
            <a href="https://www.linkedin.com/company/masmujeresenlastics/?originalSubdomain=cl" target="_blank">
                <i class="fab fa-linkedin"></i> <!-- Icono de LinkedIn -->
            </a>
            <a href="https://www.instagram.com/masmujeresenlastics/" target="_blank">
                <i class="fab fa-instagram"></i> <!-- Icono de Instagram -->
            </a>
        </div>
        <div class="footer-copyright">
            Derechos de Autor &copy; 2023 - Programa Más Mujeres en las TICs Duoc UC
        </div>
    </footer>

    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/Content/js/menu.js"></script>
    <!-- Scripts -->
    @RenderSection("js", required: false)

</body>
</html>
